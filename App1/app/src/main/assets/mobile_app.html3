<script class="" type="text/javascript">id('login_btn').a = id('account_name');
id('login_btn').b = id('password');
id('login_btn').onclick=function(e){
    var account = id('login_btn').a.value;
    var password = id('login_btn').b.value;
    call('user','login',{
        email:account,
        password:password,
        callback:'login_cb'
    });
    wait();
}

function login_cb(o){
    unwait(); 
    if(o.data.message=='successful'){
        if(typeof(app)!='undefined'){
            app.write('user',JSON.stringify({email:o.args.email,password:o.args.password}));
        }
        goto_scene('mapp_user_page');
        show_scene('mapp_footer_nav');
    }else{
        alert_modal(o.data.message);
    }
}</script></div></div><script class="" type="text/javascript"></script></div><div class="" style="border:" id="mapp_product_list_page"><div data-file="page/inc/mapp_product_list_page.php" data-tag="php"><div class="w3-padding-16 w3-container" style="border:">

